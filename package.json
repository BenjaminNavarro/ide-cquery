{
  "name": "ide-cquery",
  "main": "./lib/main",
  "version": "0.6.1",
  "description": "C++ and C language support for Atom-IDE",
  "repository": "https://github.com/rianneogi/ide-cquery",
  "license": "MIT",
  "keywords": [
    "c++",
    "c",
    "ide",
    "cquery",
    "lsp",
    "language-service-protocol"
  ],
  "engines": {
    "atom": ">=1.21.0"
  },
  "dependencies": {
    "atom-languageclient": "^0.9.3",
    "shell-quote": "^1.6.1"
  },
  "devDependencies": {
    "snazzy": "^7.0.0",
    "standard": "^10.0.3"
  },
  "standard": {
    "globals": [
      "afterEach",
      "atom",
      "beforeEach",
      "describe",
      "expect",
      "it",
      "jasmine",
      "runs",
      "spyOn",
      "waitsFor",
      "waitsForPromise"
    ]
  },
  "enhancedScopes": [
    "source.c",
    "source.cpp"
  ],
  "consumedServices": {
    "linter-indie": {
      "versions": {
        "2.0.0": "consumeLinterV2"
      }
    },
    "datatip": {
      "versions": {
        "0.1.0": "consumeDatatip"
      }
    },
    "signature-help": {
      "versions": {
        "0.1.0": "consumeSignatureHelp"
      }
    }
  },
  "providedServices": {
    "autocomplete.provider": {
      "versions": {
        "2.0.0": "provideAutocomplete"
      }
    },
    "code-format.range": {
      "versions": {
        "0.1.0": "provideCodeFormat"
      }
    },
    "code-highlight": {
      "versions": {
        "0.1.0": "provideCodeHighlight"
      }
    },
    "definitions": {
      "versions": {
        "0.1.0": "provideDefinitions"
      }
    },
    "find-references": {
      "versions": {
        "0.1.0": "provideFindReferences"
      }
    },
    "outline-view": {
      "versions": {
        "0.1.0": "provideOutlines"
      }
    },
    "code-actions": {
      "versions": {
        "0.1.0": "provideCodeActions"
      }
    }
  },
  "configSchema": {
    "cqueryPath": {
      "type": "string",
      "default": "cquery",
      "description": "Path to the cquery executable"
    },
    "cacheDirectory": {
      "type": "string",
      "default": "$projectPath/.vscode/cquery_cached_index",
      "description": "The Cquery Cache will be stored in this path"
    },
    "enableSnippetInsertion": {
      "type": "boolean",
      "default": false,
      "description": "Autocomplete options show as a snippet"
    },
    "enableLogging": {
      "type": "boolean",
      "default": false,
      "description": "Enable logging for debugging ide-cquery"
    },
    "logFile": {
      "type": "string",
      "default": "cquery_log.txt"
    },
    "extraClangArguments": {
      "type": "array",
      "default": [],
      "description": "Additional arguments to pass to clang",
      "items": {
        "type": "string"
      }
    }
  }
}
